{
  "name": "claudia-express",
  "version": "1.0.0",
  "description": "Example application for running a Node Express app on AWS Lambda using Amazon API Gateway.",
  "main": "lambda.js",
  "scripts": {
    "compile": "tsc",
    "deploy": "claudia create --handler lambda.handler --deploy-proxy-api --region ap-south-1",
    "update": "claudia update",
    "generate-proxy": "claudia generate-serverless-express-proxy --express-module build/app"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "aws-serverless-express": "^1.3.0",
    "express": "^4.14.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.51",
    "@types/express": "^4.17.7",
    "@types/node": "^13.13.5",
    "aws-sdk": "^2.655.0",
    "claudia": "^5.12.0",
    "typescript": "^3.8.3"
  }
}
